{
	"name": "df_load_location_dim",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Geo_Data_CSV",
						"type": "DatasetReference"
					},
					"name": "sourceGeoData2Location"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_EDW_location_dim",
						"type": "DatasetReference"
					},
					"name": "sinkLocation"
				}
			],
			"transformations": [
				{
					"name": "surrogateKey"
				},
				{
					"name": "derivedColumn"
				}
			],
			"scriptLines": [
				"parameters{",
				"     DI_Job_ID as string (\"59\")",
				"}",
				"source(output(",
				"          state_fips as short,",
				"          state as string,",
				"          state_abbr as string,",
				"          zipcode as string,",
				"          county as string,",
				"          city as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceGeoData2Location",
				"sourceGeoData2Location keyGenerate(output(rowNum as long),",
				"     startAt: 1L,",
				"     stepValue: 1L) ~> surrogateKey",
				"surrogateKey derive(Location_SK = rowNum,",
				"          DI_JOB_ID = $DI_Job_ID,",
				"          DI_Created_Date = toString(currentDate(),'yyyy-MM-dd')) ~> derivedColumn",
				"derivedColumn sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          LOCATION_SK as string,",
				"          ZIP as string,",
				"          CITY as string,",
				"          STATE as string,",
				"          STATE_ABBREVIATION as string,",
				"          DI_JOB_ID as string,",
				"          DI_CREATED_DT as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     stageInsert: true,",
				"     mapColumn(",
				"          LOCATION_SK = Location_SK,",
				"          ZIP = zipcode,",
				"          CITY = city,",
				"          STATE = state,",
				"          STATE_ABBREVIATION = state_abbr,",
				"          DI_JOB_ID,",
				"          DI_CREATED_DT = DI_Created_Date",
				"     )) ~> sinkLocation"
			]
		}
	}
}