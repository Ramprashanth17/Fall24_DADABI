{
	"name": "df_change_column_names_Montgomery",
	"properties": {
		"description": "Changing to ADF compliant col names for Montgomery dataset.",
		"folder": {
			"name": "Final_Project_DADABI"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_txtfile_montgomery_crashes",
						"type": "DatasetReference"
					},
					"name": "source1",
					"description": "Import data from ds_txtfile_montgomery_crashes"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_txtfile_Montgomery_corrected_columns_copy1",
						"type": "DatasetReference"
					},
					"name": "sink1",
					"description": "Export data to ds_txtfile_Montgomery_crashes_corrected_columns"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     partitionBy('hash', 1)) ~> source1",
				"source1 select(partitionBy('hash', 1),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['Montgomery_Columns_Corrected.tsv'],",
				"     umask: 0777,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}